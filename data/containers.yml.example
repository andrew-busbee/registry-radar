containers:
  - name: "nginx"
    image: "nginx"
    # tag: "latest"  # Optional - defaults to latest
    registry: "dockerhub"
  
  - name: "node"
    image: "node"
    tag: "18-alpine"
    registry: "dockerhub"
  
  - name: "redis"
    image: "redis"
    tag: "7-alpine"
    registry: "dockerhub"
  
